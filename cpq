        ScriptExecutor.ExecuteGlobal("GS_CONTACTO_JURIDICO", {'accountID':account, 'customerID':customerID, 'userCPI':userCPI, 'passwordCPI':passwordCPI, 'urlContactoC4C':urlContactoC4C })